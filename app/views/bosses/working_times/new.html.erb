<div class="container">
  <div class="row">
    <h3 class="p-3"><span class="header">勤務作成</span></h3>
  </div>

  <%= form_with model: @working_time, url: bosses_worker_working_times_path(params[:worker_id]), local: true do |f| %>

  <h4>開始時刻</h4>
  <%= f.datetime_select :start_at,{:end_year => 2030} %>

  <h4>終了時刻</h4>
  <%= f.datetime_select :finish_at,{:end_year => 2030} %>

  <%= f.submit '新規作成' %>
  <% end %>
</div>